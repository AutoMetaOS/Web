		"svelte": "3.44.0",
		"svelte-preprocess": "4.7.3",
		"vite": "2.4.1"
	},
	"type": "module",
	"dependencies": {
		"@builder.io/partytown": "^0.5.4",
		"lsbridge": "^0.2.0",
		"lz-string": "^1.4.4",
		"markdown-it": "^12.2.0",
		"predefined": "1.2.5",
		"vite-plugin-replace": "^0.1.1"
